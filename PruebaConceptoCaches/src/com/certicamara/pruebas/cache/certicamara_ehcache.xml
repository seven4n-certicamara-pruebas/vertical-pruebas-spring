	<cache name="CERTIFACTURA_CLIENTES" maxElementsInMemory="10" eternal="true"
		timeToIdleSeconds="0" timeToLiveSeconds="0" overflowToDisk="false"
		diskPersistent="false"
		memoryStoreEvictionPolicy="LRU">
	</cache>
</ehcache>